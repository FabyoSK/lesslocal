{
  "name": "@fabyosk/lesslocal",
  "version": "1.0.0-beta-7",
  "description": "Run Less locally",
  "author": "FÃ¡bio Alves @FabyoSK",
  "bin": {
    "lesslocal": "./bin/run"
  },
  "homepage": "https://github.com/FabyoSK/lesslocal",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "https://github.com/FabyoSK/lesslocal",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@inquirer/confirm": "^2.0.12",
    "@inquirer/prompts": "^3.1.1",
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^3.7.0",
    "chokidar": "^3.5.3"
  },
  "devDependencies": {
    "@oclif/test": "^2.5.4",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.52",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3.17.1",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.1",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "lesslocal",
    "dirname": "lesslocal",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "hooks": {
      "predeploy": "./dist/hooks/predeploy",
      "postdeploy": "./dist/hooks/postdeploy"
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=16.x"
  },
  "bugs": "https://github.com/FabyoSK/lesslocal/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
